{
  "name": "attainia-web-components",
  "version": "2.0.0",
  "description": "A collection of Attainia branded web components to be used in an Attainia React.js web application.",
  "main": "index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "storybook": "NODE_PATH=src start-storybook -p 9001 -c .storybook",
    "build-storybook": "NODE_PATH=src build-storybook -c .storybook -o .storybook-build",
    "build-dev": "react-scripts build",
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=production babel src/components --out-dir dist --ignore node_modules/,**/__mocks__/,**/*.test.js --copy-files",
    "clean": "node_modules/.bin/rimraf build dist",
    "copy-manifest": "node_modules/.bin/copyfiles package.json .npmrc dist/",
    "predist": "npm run build && npm run copy-manifest",
    "dist": "cd dist && npm publish",
    "precommit-validate": "hooks/lint_js || (echo 'Please fix any of the above-mentioned linting errors before finalizing this commit.' && exit 1)",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint \"src/**/*.js\"",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "pre-commit": {
    "run": [
      "precommit-validate"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Attainia/attainia-web-components.git"
  },
  "keywords": [
    "attainia",
    "react",
    "component"
  ],
  "license": "Apache-2.0",
  "author": "Attainia Developers <developers@attainia.com>",
  "contributors": [
    "Alex <alex.maskovyak@attainia.com>",
    "Ana <ana.tomboulian@attainia.com>",
    "David <david.nunez@attainia.com>",
    "Lawrence <lawrence.grant@attainia.com>",
    "Michael <michael.tomcal@attainia.com>",
    "Jono <jono.mercier@attainia.com>",
    "Glenn <glenn.siegman@attainia.com>",
    "Marcos <marcos.gonzales@attainia.com>",
    "Sean <sean.mchugh@attainia.com>",
    "Tim <tim.graf@attainia.com>",
    "Usha <usha.kundapur@attainia.com>"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x",
    "react-table": "6.x",
    "prop-types": "15.x",
    "carbon-components": "^9.10.1",
    "carbon-components-react": "^6.28.1",
    "carbon-icons": "^7.0.7",
    "lodash": "4.x.x",
    "formik": "1.x"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.15",
    "@storybook/addon-info": "3.2.13",
    "@storybook/addon-knobs": "3.2.15",
    "@storybook/addon-links": "3.2.15",
    "@storybook/addon-notes": "3.2.15",
    "@storybook/addon-storyshots": "3.2.16",
    "@storybook/react": "3.2.13",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "copyfiles": "1.2.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-import-resolver-node": "0.3.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "glob-all": "3.1.0",
    "history": "4.7.2",
    "pre-commit": "1.2.2",
    "prettier": "1.7.0",
    "prettier-eslint-cli": "4.3.2",
    "regenerator-runtime": "0.11.0",
    "rimraf": "2.6.1",
    "react": "16.x",
    "react-dom": "16.x",
    "react-table": "6.x",
    "prop-types": "15.x",
    "carbon-components": "^9.10.1",
    "carbon-components-react": "^6.28.1",
    "carbon-icons": "^7.0.7",
    "lodash": "4.x.x",
    "formik": "1.x"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "babylon",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
  }
}
